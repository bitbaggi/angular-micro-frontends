{
  "$schema": "../../node_modules/ng-packagr/package.schema.json",
  "name": "@angular-mfe/shared",
  "version": "1.0.0",
  "peerDependencies": {
    "@angular/common": "^9.0.2",
    "@angular/core": "^9.0.2",
    "@microsoft/applicationinsights-web": "^2.5.2",
    "tslib": "^1.10.0"
  },
  "ngPackage": {
    "dest": "../../dist/shared",
    "lib": {
      "entryFile": "./shared.mfe.ts",
      "umdId": "mfe.shared",
      "umdModuleIds": {
        "@microsoft/applicationinsights-web": "Microsoft.ApplicationInsights"
      }
    },
    "deleteDestPath": true
  }
